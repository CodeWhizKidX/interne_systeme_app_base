{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run script:copy-generated && npm run prisma:generate && tsc",
    "start": "node dist/server.js",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/server.js\"",
    "script:copy-generated": "ts-node src/scripts/copy-generated.ts",
    "script:convertSchema": "ts-node src/scripts/convertSchema.ts",
    "tsoa:routes": "npx tsoa routes",
    "tsoa:spec": "npx tsoa spec",
    "exec-tsoa": "npm run tsoa:routes && npm run tsoa:spec && npm run script:copy-generated",
    "prisma:push": "npx prisma db push",
    "prisma:pull": "npx prisma db pull --force",
    "prisma:generate": "npx prisma generate",
    "prisma:format": "npx prisma-case-format --file prisma/schema.prisma",
    "exec-prisma": "npm run prisma:pull && npm run prisma:format && npm run prisma:generate",
    "api:copyfront": "openapi-generator-cli generate -g typescript-axios -i src/generated/tsoa/swagger.json -o ./../frontend/src/generated",
    "exec-api": "npm run exec-tsoa && npm run api:copyfront"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.17.1",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "firebase-admin": "^13.0.2",
    "google-auth-library": "^10.4.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "pg": "^8.16.3",
    "prisma": "^6.17.1",
    "stripe": "^20.3.1",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^6.6.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.25.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.7.2",
    "@types/pg": "^8.15.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "prisma-case-format": "^2.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "description": ""
}
